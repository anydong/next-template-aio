{
  "extends": ["alloy", "prettier", "next/core-web-vitals"],
  "plugins": ["@typescript-eslint"],
  "rules": {
    "no-unused-vars": "off",
    "@typescript-eslint/no-unused-vars": [
      "error",
      {
        "vars": "all",
        "args": "after-used",
        "ignoreRestSiblings": false,
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^Metadata$"
      }
    ]
  },
  "env": {
    "jest": true
  }
}
